// JS CODE

let x;
let y;
let z;

document.getElementById("myButton").onclick = function() {
    x = document.getElementById("textA").value;
    x = Number(x);

    y = document.getElementById("textB").value;
    y = Number(y);

    z = Math.sqrt(Math.pow(x, 2) + Math.pow(y, 2));

    document.getElementById("sidec").innerHTML = "<br>" + "Side of C: " + z;
}
