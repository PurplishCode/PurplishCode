<body>
  
    <label>Side A:<input type="text" id="textA"><br></label><br>
    <label>Side B:<input type="text" id="textB"></label><br><br>
    <button type="button" id="myButton">Calculate</button>
    <label id="sidec"></label>

    
    <script src="index.js">

    </script>
</body>
